#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		long long n,k,l;
		cin>>n>>k>>l;
		if(k==1)
		{
			cout<<-1<<endl;
			continue;
		}		
		if(k*l<n)
		{
			cout<<-1<<endl;
			continue;
		}
		else
		{
			
			for(int i=1;i<=n;i++)
			{
				
					cout<<(i%(k))+1<<" ";
					
			}
				
			
			cout<<endl;
			
		}
	}
}
